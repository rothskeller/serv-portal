{
    "compilerOptions": {
        "jsx": "preserve",
        "target": "esnext",
        "module": "esnext",
        "strict": true,
        "moduleResolution": "node",
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true,
        "types": [
            "googlemaps"
        ]
    },
    "include": [
        "client/src/**/*.ts",
        "client/src/**/*.tsx",
        "client/src/**/*.vue"
    ],
    "exclude": [
        "node_modules"
    ]
}